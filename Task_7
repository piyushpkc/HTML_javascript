<script>
    function lw() {
    
    var i = document.getElementById("in1").value
    
    var xhr = new XMLHttpRequest();
    
    
    xhr.open("GET","http://1http://192.168.43.50/cgi-bin/docker.py?x="+i, true);
    xhr.send();
    
    xhr.onload = function() {
        var output = xhr.responseText;
        document.getElementById("d1").innerHTML = output;
        }
    }
    
    </script>
    

